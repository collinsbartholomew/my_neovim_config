{
	"Hello World x64": {
		"prefix": "hello64",
		"body": [
			"section .data",
			"    msg db 'Hello, World!', 0xA",
			"    msg_len equ $ - msg",
			"",
			"section .text",
			"    global _start",
			"",
			"_start:",
			"    ; write syscall",
			"    mov rax, 1",
			"    mov rdi, 1",
			"    mov rsi, msg",
			"    mov rdx, msg_len",
			"    syscall",
			"",
			"    ; exit syscall",
			"    mov rax, 60",
			"    mov rdi, 0",
			"    syscall"
		],
		"description": "Hello World program for x86-64"
	},
	"Function Template": {
		"prefix": "func",
		"body": [
			"${1:function_name}:",
			"    push rbp",
			"    mov rbp, rsp",
			"    ",
			"    ; Function body",
			"    ${2:; Your code here}",
			"    ",
			"    mov rsp, rbp",
			"    pop rbp",
			"    ret"
		],
		"description": "Standard function template with stack frame"
	},
	"System Call": {
		"prefix": "syscall",
		"body": [
			"mov rax, ${1:syscall_number}  ; ${2:syscall_name}",
			"mov rdi, ${3:arg1}",
			"mov rsi, ${4:arg2}",
			"mov rdx, ${5:arg3}",
			"syscall"
		],
		"description": "Linux x86-64 system call template"
	},
	"Loop Template": {
		"prefix": "loop",
		"body": [
			"mov rcx, ${1:count}",
			"${2:loop_start}:",
			"    ${3:; Loop body}",
			"    loop ${2:loop_start}"
		],
		"description": "Basic loop structure"
	},
	"Data Section": {
		"prefix": "data",
		"body": [
			"section .data",
			"    ${1:variable} ${2:db} ${3:value}  ; ${4:comment}"
		],
		"description": "Data section with variable"
	},
	"BSS Section": {
		"prefix": "bss",
		"body": [
			"section .bss",
			"    ${1:buffer} resb ${2:size}  ; ${3:comment}"
		],
		"description": "BSS section with uninitialized data"
	}
}
